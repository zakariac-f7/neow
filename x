    // DOM Elements
    const YEAR_SPAN = document.getElementById('year');
    const appsGrid = document.getElementById('apps-grid');
    const searchInput = document.getElementById('search');
    const carousel = document.getElementById('trending-carousel');
    const prevBtn = document.getElementById('trending-prev');
    const nextBtn = document.getElementById('trending-next');
    const modal = document.getElementById('install-modal');
    const modalClose = document.getElementById('modal-close');
    const modalInstallBtn = document.getElementById('modal-install-btn');
    const verificationPopup = document.getElementById('verificationPopup');
    const generatingScreen = document.getElementById('generatingScreen');
    const progressBar = document.getElementById('progressBar');
    const continueBtn = document.getElementById('continueBtn');
    const cancelBtn = document.getElementById('cancelBtn');

    // Set current year
    YEAR_SPAN.textContent = new Date().getFullYear();

    // Initialize trending carousel
    function initTrendingCarousel() {
      const trendingApps = APPS.filter(app => app.trending);
      carousel.innerHTML = '';
      
      trendingApps.forEach(app => {
        const rating = randomRating(4, 5);
        const downloads = randomInt(300, 500);
        const item = document.createElement('div');
        item.className = 'trending-item';
        
        item.innerHTML = `
          ${app.badge ? `<span class="trending-item-badge">${app.badge</span>` : ''}
          <img src="${app.icon}" alt="${app.name}" class="trending-item-img" loading="lazy">
          <div class="trending-item-info">
            <div class="trending-item-title">${app.name}</div>
            <div class="trending-item-stats">
              <span class="trending-rating">${'★'.repeat(rating)}${'☆'.repeat(5 - rating)}</span>
              <span class="download-count">
                <svg width="14" height="14" viewBox="0 0 24 24" fill="none" stroke="currentColor" stroke-width="2" stroke-linecap="round" stroke-linejoin="round">
                  <path d="M21 15v4a2 2 0 0 1-2 2H5a2 2 0 0 1-2-2v-4"></path>
                  <polyline points="7 10 12 15 17 10"></polyline>
                  <line x1="12" y1="15" x2="12" y2="3"></line>
                </svg>
                ${downloads}K
              </span>
            </div>
            <button class="btn-install" data-link="${app.locker_id}" data-name="${app.name}" data-icon="${app.icon}">
              <svg width="16" height="16" viewBox="0 0 24 24" fill="none" stroke="currentColor" stroke-width="2" stroke-linecap="round" stroke-linejoin="round" style="margin-right: 4px;">
                <path d="M21 15v4a2 2 0 0 1-2 2H5a2 2 0 0 1-2-2v-4"></path>
                <polyline points="7 10 12 15 17 10"></polyline>
                <line x1="12" y1="15" x2="12" y2="3"></line>
              </svg>
              <span class="install-btn-text"><b>INSTALL</b></span>
            </button>
          </div>
        `;
        carousel.appendChild(item);
      });
      
      // Carousel navigation
      prevBtn.addEventListener('click', () => {
        carousel.scrollBy({ left: -200, behavior: 'smooth' });
      });
      
      nextBtn.addEventListener('click', () => {
        carousel.scrollBy({ left: 200, behavior: 'smooth' });
      });
      
      // Disable buttons when at ends
      const updateNavButtons = () => {
        prevBtn.disabled = carousel.scrollLeft <= 10;
        nextBtn.disabled = carousel.scrollLeft >= carousel.scrollWidth - carousel.clientWidth - 10;
      };
      
      carousel.addEventListener('scroll', updateNavButtons);
      updateNavButtons();
    }

    // Render all apps grid
    function renderApps(list){
      appsGrid.innerHTML = '';
      list.forEach(app => {
        const rating = randomRating(4, 5);
        const downloads = randomInt(100, 400);
        const stars = toStars(rating);
        const card = document.createElement('div');
        card.className = 'card';
        card.setAttribute('data-name', app.name.toLowerCase());

        card.innerHTML = `
          <div class="app-top">
            <img class="app-icon" src="${app.icon}" alt="${app.name} icon" loading="lazy">
            <div class="app-name">${app.name}</div>
          </div>

          <div class="platforms">${renderPlatforms(app.platforms)}</div>

          <div class="rating-row">
            <span class="rating-stars" aria-label="${rating} stars out of 5">${stars}</span>
            <span class="downloads">
              <svg width="14" height="14" viewBox="0 0 24 24" fill="none" stroke="currentColor" stroke-width="2" stroke-linecap="round" stroke-linejoin="round">
                <path d="M21 15v4a2 2 0 0 1-2 2H5a2 2 0 0 1-2-2v-4"></path>
                <polyline points="7 10 12 15 17 10"></polyline>
                <line x1="12" y1="15" x2="12" y2="3"></line>
              </svg>
              ${downloads}K
            </span>
          </div>

          <button class="btn-install" data-link="${app.locker_id}" data-name="${app.name}" data-icon="${app.icon}">
            <svg width="16" height="16" viewBox="0 0 24 24" fill="none" stroke="currentColor" stroke-width="2" stroke-linecap="round" stroke-linejoin="round" style="margin-right: 4px;">
              <path d="M21 15v4a2 2 0 0 1-2 2H5a2 2 0 0 1-2-2v-4"></path>
              <polyline points="7 10 12 15 17 10"></polyline>
              <line x1="12" y1="15" x2="12" y2="3"></line>
            </svg>
            <span class="install-btn-text"><b>INSTALL</b></span>
          </button>
        `;
        appsGrid.appendChild(card);
      });
    }

    // Helper functions
    function renderPlatforms(arr){
      return arr.map(p => `
        <span class="platform">
          ${p === 'android' ? androidIcon() : p === 'ios' ? iosIcon() : windowsIcon()} ${p.toUpperCase()}
        </span>
      `).join('');
    }

    function androidIcon(){
      return `<svg width="14" height="14" viewBox="0 0 24 24" fill="#3ddc84" xmlns="http://www.w3.org/2000/svg">
        <path d="M17.6 9.48l1.43-2.48-.87-.5-1.5 2.61a7.99 7.99 0 00-7.32 0L5.84 6.5l-.87.5 1.43 2.48C4.9 10.79 4 12.8 4 15v3c0 .55.45 1 1 1h2v3a1 1 0 001 1h1a1 1 0 001-1v-3h2v3a1 1 0 001 1h1a1 1 0 001-1v-3h2c.55 0 1-.45 1-1v-3c0-2.2-.9-4.21-2.4-5.52zM8.5 7c.56 0 1-.45 1-1s-.44-1-1-1a1 1极速加速器 极速加速器 0 00-1 1c0 .55.45 1 1 1zm7 0c.56 0 1-.45 1-1s-.44-1-1-1a1 1 0 00-1 1c0 .55.45 1 1 1z"/>
      </svg>`;
    }

    function iosIcon(){
      return `<svg width="14" height="14" viewBox="0 0 24 24" fill="#ffffff" xmlns="http://www.w3.org/2000/svg">
        <path d="M16.365 1.43c0 1.14-.46 2.275-1.22 极速加速器 3.08-.784.83-2.06 1.487-3.17 1.44-.152-1.09.507-2.27 1.256-3.03.808-.83 2.17-1.43 3.13-1.49.02.02.02.02.02 0 .003.02.003.02.003 0-.003 0-.003 0-.003 0h-.016zM20.48 17.47c-.337.76-.49 1.13-.896 1.83-.58.996-1.4 2.223-2.408 2.24-.885.02-1.164-.57-2.29-.57-1.122 0-1.43.555-2.31.59-1.02.038-1.8-1.08-2.38-2.07-1.3-2.2-2.28-6.21-.954-8.92.66-1.37 1.83-2.24 3.127-2.26.975-.02 1极速加速器.89.65 2.51.65.615 0 1.73-.8 2.92-.69.498.02 1.894.2 2.79 1.51-.073.05-1.666.98-1.65 2.93.02 2.34 2.034 3.12 2.06 3.13-.02.05-.312 1.07-.522 1.58z"/>
      </svg>`;
    }

    function windowsIcon(){
      return `<svg width="14" height="14" viewBox="0 0 24 24" fill="#0078d7" xmlns="http://www.w3.org/2000/svg">
        <path d="M7,6h15c0.552,0,1,0.448,1,1极速加速器v15c0,0.552-0.448,1-1,1H7c-0.552,0-1-0.448-1-1V7 C6,6.448,6.448,6极速加速器,7,6z" />
        <path d="M25.042,21.958V7c0-0.552,0.448-1,1-1H41c0.552,0,1,0.448,1,1v14.958 c0,0.552-0.448,1-1,1H26.042C25.489,22.958,25.042,22.511,25.042,21.958z" />
        <path d="M7,25h15c0.552,0,1,0.448,1,1v15c0,0.552-极速加速器0.448,1-1,1H7c-0.552,0-1-极速加速器0.448-1-1V26 C6,25.448,6.448,25,7,25z" />
        <path d="M25,41V26c0-0.552,0.448-1,1-1h15c0.552,0,1,0.448,1,1v15c0,0.552-0.448,1-1,1H26 C25.448,42,25,41.552,25,41z" />
      </svg>`;
    }

    function toStars(rating){
      return '★'.repeat(rating) + '☆'.repeat(5 - rating);
    }

    function randomRating(min=4,max=5){
      return Math.random() > 0.5 ? 5 : 4;
    }

    function randomInt(min,max){
      return Math.floor(Math.random()*(max-min+1))+min;
    }

    // Verification flow
    let currentLink = '';
    
    function showVerification(link, name, icon) {
      currentLink = link;
      document.getElementById('verificationPopup').style.display = 'flex';
      
      // Update modal content if provided
      if (name && icon) {
        document.getElementById('app_icon').极速加速器src = icon;
        document.getElementById('app_name').textContent = name;
        document.getElementById('app-caption-title').textContent = name;
      }
    }
    
    continueBtn.addEventListener('click', function() {
      // Hide verification popup
      document.getElementById('verificationPopup').style.display = 'none';
      
      // Show generating screen
      generatingScreen.style.display = 'flex';
      progressBar.style.width = '100%';
      
      // Redirect after 3 seconds
      setTimeout(function() {
        window.location.href = currentLink;
      }, 3000);
    });
    
    cancelBtn.addEventListener('click', function() {
      document.getElementById('verificationPopup').style.display = 'none';
    });

    // Install modal functions
    function install(icon, name, link) {
      document.getElementById("app_icon").src = icon;
      document.getElementById("app_name").textContent = name;
      document.getElementById("app-caption-title").textContent = name;
      
      // Set up the modal install button
      modalInstallBtn.onclick = function() {
        closeModal();
        showVerification(link, name, icon);
      };

      // Show modal
      modal.classList.add('active');
      document.body.style.overflow = 'hidden';
    }

    function closeModal(){
      modal.classList.remove('active');
      document.body.style.overflow = '';
    }

    // Event listeners
    modalClose.addEventListener('click', closeModal);
    modal.addEventListener('click', (e) => {
      if(e.target === modal) closeModal();
    });
    document.addEventListener('keydown', (e) => {
      if(e.key === 'Escape') closeModal();
    });

    // Search functionality
    searchInput.addEventListener('input', (极速加速器e) => {
      const q = e.target.value.toLowerCase().trim();
      const cards = Array.from(appsGrid.children);
      cards.forEach(card => {
        const name = card.getAttribute('data-name');
        card.style.display = name.includes(q) ? '' : 'none';
      });
    });

    // Add event listeners to all install buttons
    document.addEventListener('click', function(e) {
      if (e.target.classList.contains('btn-install')) {
        const link = e.target.getAttribute('data-link');
        const name = e.target.getAttribute('data-name');
        const icon = e.target.getAttribute('data-icon');
        
        if (link) {
          install(icon, name, link);
        }
      }
    });

    // Initialize everything
    initTrendingCarousel();
    renderApps(APPS);

    // VPN check
    (async function() {
      try {
        const ipRes = await fetch('https://api.ipify.org?format=json');
        const ipData = await ipRes.json();
        const vpnRes = await fetch('https://vpnapi.io/api/' + ipData.ip + '?key=d4f2116e2ff14极速加速器e6abf3ecd0ae5d96667');
        const vpnData = await vpn极速加速器Res.json();

        if (vpnData.security && (vpnData.security.vpn || vpnData.security.proxy)) {
          document.body.innerHTML = "<h1 style='color: red; text-align: center; margin-top: 50px;'>Access is Blocked: No VPN or Proxy Allowed!</h1>";
        }
      } catch (error) {
        console.error("VPN check failed:", error);
      }
    })();
  </script>
</body>
</html>
